{
    "editor.renderWhitespace": "boundary",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.wordWrap": "on",
    "editor.tabCompletion": true,
    "editor.rulers": [
        80
    ],
    "editor.roundedSelection": false,
    "editor.formatOnPaste": true,
    "editor.emptySelectionClipboard": false,
    "editor.cursorBlinking": "expand",
    "editor.showFoldingControls": "always",
    "editor.accessibilitySupport": "off",
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}",
    "window.newWindowDimensions": "inherit",
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "php.validate.executablePath": "/usr/bin/php",
    "phpformatter.pharPath": "/usr/local/bin/php-cs-fixer",
    "markdown.previewFrontMatter": "show",
    "markdown-preview-enhanced.scrollSync": false,
    "git.confirmSync": false,
    "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\wsl.exe",
    "workbench.colorTheme": "Hopscotch",
    "markdown-preview-enhanced.enableTypographer": true,
    "markdown-preview-enhanced.mermaidTheme": "mermaid.forest.css",
    "markdown-preview-enhanced.codeBlockTheme": "hopscotch.css",
    "markdown-preview-enhanced.previewTheme": "night.css",
    "markdown-preview-enhanced.revealjsTheme": "black.css",
    "window.zoomLevel": 0,
    "workbench.editor.tabCloseButton": "off",
    "python.pythonPath": "python3",
    "workbench.iconTheme": "vscode-icons",
    "git.autofetch": true
}
